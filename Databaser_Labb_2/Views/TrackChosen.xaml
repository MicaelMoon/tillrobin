<UserControl x:Class="Databaser_Labb_2.Views.TrackChosen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Databaser_Labb_2.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button
        Grid.ColumnSpan="2"
        Width="130" Height="50"
        Content="Delete Track"
        FontSize="15"
        x:Name="DeleteTrackBtn" Click="DeleteTrackBtn_Click"
        />
        <Button
Grid.ColumnSpan="2"
Width="130" Height="50"
Content="Test Track"
FontSize="15"
x:Name="TestTrackBtn" Click="Test"/>

    </Grid>
</UserControl>
